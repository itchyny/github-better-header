{
  "manifest_version": 2,
  "name": "GitHub Better Header",
  "version": "0.0",
  "author": "itchyny",
  "content_scripts": [ {
    "all_frames": false,
    "js": [ "github-better-header.js" ],
    "css": [ "style.css" ],
    "run_at": "document_start",
    "matches": [ "https://github.com/*" ]
  } ],
  "web_accessible_resources": [ "*.html" ]
}
